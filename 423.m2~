R = QQ[u,t]
M = matrix{
    {-3*u,0,1,1,1,0,0,0},
    {0,-3,0,0,0,u,u,u},
    {u,0,-3-2*u-t,2*t,1,1,0,1},
    {u,0,2,-3-2*t-2*u,2*t,1,1,0},
    {u,0,t,2,-2-2*t-2*u,0,1,1},
    {0,1,u,u,0,-4-t-u,t,1+t},
    {0,1,0,u,u,1,-3-2*t-u,1+t},
    {0,1,u,0,u,1+t,1+t,-4-u-2*t}}
v = gens ker M
d = gcd(flatten(entries v))
print(d, v//d)
